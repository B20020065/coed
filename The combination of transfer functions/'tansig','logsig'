p=p'; 
[P,ps1]=mapminmax(p,0,1); 
T=t'; 
 [m,n]=size(P); 
P_train=P(:,1:n-10); 
P_test=P(:,n-9:n); 
 [m,n]=size(T);
T_train=T(:,1:n-10); 
T_test=T(:,n-9:n); 
net_1 = newelm(minmax(P_train),[9,1], {'tansig','logsig'} ,'traingdm'); 
net_1.trainParam.show = 50; 
net_1.trainParam.lr = 0.01; 
net_1.trainParam.mc = 0.9; 
net_1.trainParam.epochs =10000; 
net_1.trainParam.goal = 1e-3; 
net=init(net_1); 
net = train(net,P_train,T_train);  
t_train= sim(net,P_train) 
t_test= sim(net,P_test) 
w1=net.iw{1,1} 
theta1=net.b{1} 
w2=net.lw{2,1} 
theta2=net.b{2}
